FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Qdot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    right
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    wallsY
    {
        type            calculated;
        value           uniform 0;
    }
    wallsZ
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to4
    {
        type            processor;
        value           uniform 0;
    }
}


