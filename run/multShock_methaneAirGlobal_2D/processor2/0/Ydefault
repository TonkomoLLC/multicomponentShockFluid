FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ydefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            zeroGradient;
    }
    right
    {
        type            waveTransmissive;
        gamma           1.4;
        fieldInf        0;
        lInf            1000;
        value           nonuniform List<scalar> 0();
    }
    wallsY
    {
        type            zeroGradient;
    }
    wallsZ
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0;
    }
}


